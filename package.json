{
	"name": "WebKeyNote",
	"displayName": "WebKeyNote",
	"description": "make slide by coding.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.32.0"
	},
	"categories": [
		"Other",
		"Snippets"
	],
	"keywords": [
		"WebKeyNote",
		"ppt",
		"idea"
	],
	"activationEvents": [
		"onCommand:extension.WebKeyNote",
		"onCommand:extension.open_doc",
		"onCommand:extension.open_section",
		"onCommand:extension.open_doc_editor",
		"onCommand:extension.open_section_editor",
		"onView:WebKeyNote_current_doc",
		"onView:WebKeyNote_doc_template",
		"onView:WebKeyNote_doc_dir"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.WebKeyNote",
				"category": "WebKeyNote",
				"title": "新建幻灯片"
			},{
				"command": "extension.open_doc",
				"category": "WebKeyNote",
				"title": "open doc"
			},{
				"command": "extension.open_section",
				"category": "WebKeyNote",
				"title": "open section"
			},{
				"command": "extension.open_doc_editor",
				"category": "WebKeyNote",
				"title": "edit doc"
			},{
				"command": "extension.open_section_editor",
				"category": "WebKeyNote",
				"title": "edit section"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "package-WebKeyNote",
					"title": "WebKeyNote",
					"icon": "resources/keynote.svg"
				}
			]
		},
		"views": {
			"package-WebKeyNote": [
				{
					"id": "WebKeyNote_current_doc",
					"name": "当前文档"
				},
				{
					"id": "WebKeyNote_doc_dir",
					"name": "工作目录"
				},
				{
					"id": "WebKeyNote_doc_template",
					"name": "页面模版"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "yarn run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}