{
	"name": "klang-theme",
	"type": "dark",
	"colors": {
		"editor.background": "#222121c9",
		"editor.foreground": "#eeffff",
		"activityBarBadge.background": "#007acc",
		"sideBarTitle.foreground": "#bbbbbb"
	},
	"tokenColors": [
		{
			"name": "section页面",
			"scope": [
				"klang.section"
			],
			"settings": {
				"fontStyle":"bold underline",
				"foreground": "#ce900c"
			}
		},{
			"name": "页面节点",
			"scope": [
				"klang.section-node"
			],
			"settings": {
				"fontStyle":"bold",
				"foreground": "#0cce36"
			}
		},{
			"name": "属性",
			"scope": [
				"klang.attribute"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#75736e"
			}
		},{
			"name": "标点符号",
			"scope": [
				"klang.punctuation"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#f0cfba"
			}
		},{
			"name": "结点成员",
			"scope": [
				"klang.node-key"
			],
			"settings": {
				"foreground": "#61bb77"
			}
		},{
			"name": "inner结点成员",
			"scope": [
				"klang.inner-node-key"
			],
			"settings": {
				"foreground": "#222cbe"
			}
		}, {
			"name": "注释",
			"scope": [
				"klang.comments"
			],
			"settings": {
				"foreground": "#168645",
				"fontStyle": "italic"
			}
		}, {
			"name": "字符串",
			"scope": [
				"klang.string"
			],
			"settings": {
				"foreground": "#bd5d1d",
				"fontStyle": "italic"
			}
		}, {
			"name": "字符串引号",
			"scope": [
				"klang.string.quo"
			],
			"settings": {
				"foreground": "#b38c0c",
				"fontStyle": "italic"
			}
		}, {
			"name": "数字",
			"scope": [
				"klang.number"
			],
			"settings": {
				"foreground": "#282bd7"
			}
		}, {
			"name": "布尔型",
			"scope": [
				"klang.boolean"
			],
			"settings": {
				"foreground": "#b81188"
			}
		}, {
			"name": "错误",
			"scope": [
				"klang.error"
			],
			"settings": {
				"foreground": "#c00e0e",
				"fontStyle": "italic underline"
			}
		}
	]
}